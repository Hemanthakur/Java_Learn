import java.util.Scanner;

public class Pattern1{

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the no. of row in a pattern: ");
		int p=sc.nextInt();
		for(int i=1;i<=4;i++)
		{
			for(int j=1;j<=4;j++)
			{
				System.out.print("* ");
			}
		System.out.println("\n");
		}
		
	}

}
